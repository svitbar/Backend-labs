generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DBConnLink")
}

model User {
  id    Int     @default(autoincrement()) @id
  name  String?
}

model Category {
  id    Int     @default(autoincrement()) @id
  name  String?
}

model Record {
  id          Int     @default(autoincrement()) @id
  userId      Int
  categoryId  Int
  date        DateTime
  price       Float
}
